<div class="detail-table-item kit" data-field="kit">
	<div class="grid" id="kit-table" data-urls='{"read":"/epc/detail/getKit?partNo=<%= partInfo.partNo %>"}'>
		<table>
			<thead>
				<tr>
					<th>序号</th>
					<th>隶属总成编码</th>
					<th>隶属总成编码名称</th>
					<th>维修包编码</th>
					<th>维修包名称</th>
					<th>维修包备注</th>
					<th>操作</th>
				</tr>
			</thead>
			<tbody data-id="grid-result">
				<script type="text/template">
					{{#list}}
					<tr>
						<td title="{{rowNumber}}">{{rowNumber}}</td>
						<td title="{{partCode}}">{{partCode}}</td>
						<td title="{{partName}}">{{partName}}</td>
						<td title="{{code}}">{{code}}</td>
						<td title="{{name}}">{{name}}</td>
						<td title="{{note}}">{{note}}</td>
						<td>
							<% if(viewBag.hasOperation(viewBag.authCodes, 'epc:*', 'epc:detail:*','epc:detail:pkg:det')) { %>
							<a class="text-link" data-action="view-detail" data-code="{{code}}" href="javascript:;">查看详情</a>
							<% } %>
						</td>
					</tr>
					{{/list}}
				</script>
			</tbody>
		</table>
	</div>
</div>