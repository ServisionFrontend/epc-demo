	<div class="layer notice-search-layer" id="notice-dialog">
		<div class="layer-title">
			<span class="page-title"><span class="text">通知</span></span>
			<a class="layer-close-btn" href="javascript:;"></a>
		</div>
		<div class="layer-cont-wrapper">
			<div id="notice-query">
				<div class="grid-search clearfix">
					<div class="data-item">
						<span class="label">
							<span title="通知编码" class="text">通知编码：</span>
						</span>
						<input data-name="code" type="text" class="input-box">		
					</div>
					<div class="data-item">
						<span class="label">
							<span title="通知主题" class="text">通知主题：</span>
						</span>
						<input data-name="subject" type="text" class="input-box">		
					</div>
					<div class="data-item">
						<span class="label">
							<span title="通知类型" class="text">通知类型：</span>
						</span>
						<span class="form-select">
					        <input type="hidden" data-type="select" data-notClearOption="true" autocomplete="off" data-name="typeCode" value="" class="form-select-input">
					        <a href="javascript:;" class="form-select-switch-btn">
					        	<span title="" class="form-select-text">请选择</span>
					        </a>
					        <ul style="display: none;" class="form-select-option-list">
					        </ul>
					     </span>
					</div>
					<div class="data-item">
						<span class="label">
							<span title="是否已读" class="text">是否已读：</span>
						</span>
						<span class="form-select">
					        <input type="hidden" data-type="select" data-notClearOption="true" autocomplete="off" data-name="read" value="" class="form-select-input">
					        <a href="javascript:;" class="form-select-switch-btn">
					        	<span title="" class="form-select-text">请选择</span>
					        </a>
					        <ul style="display: none;" class="form-select-option-list">
					        	<li title="请选择" data-value="" class="form-select-option">请选择</li>
					        	<li title="是" data-value="1" class="form-select-option">是</li>
					        	<li title="否" data-value="0" class="form-select-option">否</li>
					        </ul>
					     </span>	
					</div>
					<div class="data-item">
						<span class="label">
							<span title="发布时间" class="text">发布时间：</span>
						</span>
						<input class="input-box data-input-box" id="notice-start-date" type="text" data-name="issueAfter">
						<span class="text-block date-input-splite">-</span>
						<input class="input-box data-input-box" id="notice-end-date" type="text" data-name="issueBefore">
					</div>
				</div>
				<div class="layer-operation">
					<a class="btn" href="javascript:;" data-id="search">查询</a>
					<a class="btn" href="javascript:;" data-id="reset">重置</a>
					<a class="text-link notice-set" href="javascript:;" data-itemId="all-set-already">全部设为已读</a>
				</div>
			</div>
			<div class="grid" id="notice-grid" data-urls='{"read":"/epc/notice"}'>
				<table id="notice-grid-table">
					<thead>
						<tr>
							<th>序号</th>
							<th>通知编码</th>
							<th>通知主题</th>
							<th>通知类型</th>
							<th>是否已读</th>
							<th>发布时间</th>
							<th>操作</th>
						</tr>
					</thead>
					<tbody data-id="grid-result">
						<script type="text/template">
						{{#list}}
							<tr data-rownumber="{{rowNumber}}">
								<td title="{{rowNumber}}">{{rowNumber}}</td>
								<td title="{{code}}">
									<% if(viewBag.hasOperation(viewBag.authCodes,'epc:*','epc:inf:*','epc:inf:download')) { %>
									<a href="{{filePath}}" class="text-link">{{code}}</a>
									<% } else {%>
									<span>{{code}}</span>
									<% } %>
								</td>
								<td title="{{subject}}">{{subject}}</td>
								<td title="{{typeName}}">{{typeName}}</td>
								<td title="{{readName}}">{{readName}}{{test}}</td>
								<td title="{{date}}">{{date}}</td>
								<td>
									{{^read}}<a href="javascript:;" data-action="set-already" class="text-link">设为已读</a>{{/read}}
								</td>
							</tr>
						{{/list}}
						</script>
					</tbody>
				</table>
			</div>

			<div class="paging clearfix" id="notice-paging">
				<div class="page-info">
					<span class="records">共<span data-id="recordCount">0</span>条记录</span>
					<span class="slash">/</span>
					<span class="page-count">共<span data-id="pageCount">0</span>页</span>
					<span class="current">（当前第<span data-id="curPageView">1</span>页）</span>
				</div>
				<div class="pagination" data-id="btns">
				    <a data-id="top" class="page-ctrl-btn top" href="javascript:void(0);"></a>
				    <a data-id="prev" class="page-ctrl-btn prev" href="javascript:void(0);"></a>
				    <a data-id="" class="text-link" href="javascript:void(0);"></a>
				    <span class="page-btns" data-id="pageBtns">
				    </span>
				    <a data-id="next" class="page-ctrl-btn next" href="javascript:void(0);"></a>
				    <a data-id="bottom" class="page-ctrl-btn bottom" href="javascript:void(0);"></a>
				    <input data-id="curPage" class="input-box page-input" type="text">
				    <a data-id="go" class="btn goto-btn" href="javascript:;">跳转</a>
				</div>
			</div>
		</div>
	</div>